{
    "extends": "@becklyn/tsconfig/nextjs.json",
    "compilerOptions": {
        "baseUrl": ".",
        "module": "commonjs",
        "outDir": "./dist",
        "moduleResolution": "node",
        "emitDeclarationOnly": true,
        "paths": {
            "@/migrations/*": ["./migrations/*"]
        }
    },
    "include": ["."],
    "exclude": ["node_modules", "dist", "test"]
}

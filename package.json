{
    "name": "@becklyn/contentful-migration",
    "version": "0.2.4",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 0",
        "lint": "eslint . --max-warnings 0",
        "format": "eslint . --fix",
        "build": "npm run clean && swc . -d dist && tsc && chmod +x dist/bin/migrate.js",
        "clean": "rm -rf dist",
        "prepublishOnly": "npm run lint && npm run build",
        "preversion": "npm run lint",
        "migrate": "node dist/bin/migrate.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Becklyn-Studios/contentful-migration.git"
    },
    "files": [
        "dist/**/*"
    ],
    "main": "dist/src/index.js",
    "types": "dist/src/index.d.ts",
    "bin": {
        "migrate": "dist/bin/migrate.js"
    },
    "dependencies": {
        "contentful-management": "^11.54.4",
        "contentful-migration": "^4.29.1",
        "dotenv": "^17.2.1",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0"
    },
    "devDependencies": {
        "@becklyn/prettier": "^2.1.5",
        "@dword-design/eslint-plugin-import-alias": "^6.0.3",
        "@next/eslint-plugin-next": "^15.4.6",
        "@swc/cli": "^0.7.8",
        "@swc/core": "^1.13.3",
        "eslint": "^9.32.0",
        "eslint-config-next": "^15.4.6",
        "next": "^15.4.6"
    },
    "prettier": "@becklyn/prettier"
}
